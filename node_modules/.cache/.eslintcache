: 3}\n    ]\n\n    return (\n        <div className=\"currencyStyle\">\n            <span>ВАЛЮТА</span>\n            <div className=\"btnCur\">\n                {currencyButtons.map((button) => (\n                <CurrencyButton key={button.id} {...button} selectedCurrency={currency}/>\n                ))}\n            </div>\n            <ul className=\"transfer\">\n                <span >КОЛИЧЕСТВО ПЕРЕСАДОК</span>\n                <FilterItem label='Все' checked={filters.every(filter => filter.checked)}/>\n                {filters.map((filter) => (\n                    <FilterItem key={filter.id} {...filter} onClick={(id) => dispatch(filterActions(id))}>\n                        <button className=\"text_2\" onClick={() =>{dispatch(getOnlyFilter(filter.id))}}>ТОЛЬКО</button>\n                    </FilterItem>\n                ))}\n            </ul>\n        </div>\n    )\n}\n\nexport default Currency;\n","/home/smissuser21/React_Project/aviasales/src/Components/Tickets.js",[],"/home/smissuser21/React_Project/aviasales/src/Redux/currencyReducer.js",[],"/home/smissuser21/React_Project/aviasales/src/Redux/actions.js",[],"/home/smissuser21/React_Project/aviasales/src/Components/FilterItem.js",[],"/home/smissuser21/React_Project/aviasales/src/Components/CurrencyButton.js",[],"/home/smissuser21/React_Project/aviasales/src/Components/ModalWindowSuccess.js",[],"/home/smissuser21/React_Project/aviasales/src/Redux/actionNames.js",["68"],"export default {\n    CURRENCY_CHANGE: 'CURRENCY_CHANGE',\n    EXCHANGE_RATES: 'EXCHANGE_RATES',\n    FILTERS: 'FILTERS',\n    ONLY_FILTER: 'ONLY_FILTER',\n    CHECK_ALL: 'CHECK_ALL',\n}","/home/smissuser21/React_Project/aviasales/src/Components/ModalWindow.js",[],"/home/smissuser21/React_Project/aviasales/src/Components/InputField.js",[],"/home/smissuser21/React_Project/aviasales/src/Components/validator.js",[],{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":21,"column":